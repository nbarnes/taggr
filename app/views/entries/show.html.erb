<p id="notice"><%= notice %></p>
<div class='entry-title'>
  <%= @entry.title %>
</div>
<div class='entry-content'>
  <%= @entry.content %>
</div>
<b>Tags:</b>
<%= @entry.tag_list.each do |tag| %>
  <%= link_to tag.name, tag_path(tag) %>
<%= end %>
</br>

<%= link_to 'Edit', edit_entry_path(@entry) %> |
<%= link_to 'Back', entries_path %>
